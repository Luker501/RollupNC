include "../circomlib/circuits/mimc.circom";

template VoteLeaf() {

    signal input x;
    signal input y;
    signal input private_key;
    signal input vote;

    signal output out;

    component balanceLeaf = MultiMiMC7(5,91);
    balanceLeaf.in[0] <== x;
    balanceLeaf.in[1] <== y;
    balanceLeaf.in[2] <== private_key;
    balanceLeaf.in[3] <== vote; 

    out <== balanceLeaf.out;
}
